{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/app-home.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "jsonquestion/**/*",
    "js/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "js": {
        "minify": false
      },
      "addServiceWorker": false,
      "name": "default"
    }
  ]
}